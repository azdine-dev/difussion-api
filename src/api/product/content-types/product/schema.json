{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "product",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string"
    },
    "slug": {
      "type": "uid",
      "targetField": "name"
    },
    "sm_pictures": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "short_desc": {
      "type": "string"
    },
    "price": {
      "type": "decimal"
    },
    "sale_price": {
      "type": "decimal"
    },
    "vendor": {
      "type": "string"
    },
    "brands": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::brand.brand",
      "inversedBy": "products"
    },
    "review": {
      "type": "integer",
      "default": 0
    },
    "ratings": {
      "type": "integer",
      "default": 0,
      "max": 5
    },
    "until": {
      "type": "date"
    },
    "stock": {
      "type": "integer"
    },
    "top": {
      "type": "boolean"
    },
    "features": {
      "type": "boolean"
    },
    "new": {
      "type": "boolean"
    },
    "variants": {
      "type": "component",
      "repeatable": true,
      "component": "variants.variants"
    },
    "author": {
      "type": "string"
    },
    "sold": {
      "type": "integer"
    },
    "pictures": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "categories": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::product-category.product-category",
      "mappedBy": "products"
    }
  }
}
